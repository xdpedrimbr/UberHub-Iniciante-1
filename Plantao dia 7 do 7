int main()
{
    int n;
    int m3 = 0, m2 = 0, m4 = 0;
    int numeros;

    scanf("%d", &n);

    for(int i = 0; i < n; i++){
        scanf("%d", &numeros);

        if(numeros % 2 == 0){
            m2++;
        }

        if(numeros % 3 == 0){
            m3++;
        }

        if(numeros % 4 == 0){
            m4++;
        }
    }

    printf("%d\n", m2);
    printf("%d\n", m3);
    printf("%d\n", m4);


    return 0;
}
